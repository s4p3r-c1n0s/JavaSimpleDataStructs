<project>

    <target name="clean">
        <delete dir="bin"/>
        <delete dir="jar"/>
    </target>

    <target name="compile">
        <mkdir dir="bin"/>
        <javac srcdir="src" destdir="bin"/>
    </target>

    <target name="jar">
        <mkdir dir="jar"/>
        <jar destfile="jar/Tree.jar" basedir="bin">
            <manifest>
				<attribute name="Main-Class" value="orderprinting.graph.AdjList"/>
            </manifest>
        </jar>
    </target>

    <target name="run">
        <java jar="jar/Tree.jar" fork="true"/>
    </target>

</project>
